    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/tove/activity#ActivityStatus -->

    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/tove/activity#ActivityStatus">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/tove/activity#completed"/>
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/tove/activity#dormant"/>
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/tove/activity#executing"/>
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/tove/activity#reExecuting"/>
                    <rdf:Description rdf:about="http://ontology.eil.utoronto.ca/tove/activity#suspended"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#ActivityThing"/>
        <terms:description xml:lang="en">Activity Status
Dormant - the activity is not executing.
Executing - the activity is enabled and executing.
Suspended - the activity was executing and has been forced to a suspended state due to dis-enabling of its state.
Terminated - the activity has finished executing.</terms:description>
    </owl:Class>
    


    <!-- http://ontology.eil.utoronto.ca/tove/activity#ActivityThing -->

