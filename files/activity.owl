    <owl:Class rdf:about="http://ontology.eil.utoronto.ca/tove/activity#AggregateActivity">
        <rdfs:subClassOf rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#elaborationOf"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#finalActivity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#initialActivity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Defines the decomposition of another activity into sub activities.
• initialActivity links to the initial activities in the decomposition.
• finalActivity links to the final activities in the decomposition.
• elaborationOf links back to the abstract activity.</rdfs:comment>
    </owl:Class>
    


