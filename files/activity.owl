            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#hasSubActivity"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#nextActivity"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasTime"/>
