            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#actBefore"/>
                <owl:allValuesFrom rdf:resource="http://ontology.eil.utoronto.ca/tove/activity#Activity"/>
